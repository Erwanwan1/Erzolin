A,CAWA,UAAA,IAAA,EAAA;AACA,QAAA,IAAA,CAAA,OAAA,CAAA,UAAA,EAAA,EAAA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,IAAA,CAAA,EAAA;AACA,SAFA;AAGA,OAfA;AAgBA;AA7DA,GA7BA;AA4FA,EAAA,OA5FA,qBA4FA;AACA,QAAA,KAAA,MAAA,CAAA,KAAA,CAAA,WAAA,EAAA;AACA,WAAA,SAAA;AACA,WAAA,UAAA;AACA,WAAA,WAAA;AACA;AACA;AAlGA,CAAA","sourcesContent":["<template>\r\n  <v-app>\r\n    <customLogin v-if=\"!$store.state.isConnected\"></customLogin>\r\n    <div v-else>\r\n      <customHeader title='Home'>\r\n      </customHeader>\r\n      <v-app-bar\r\n      max-height=\"60px\"\r\n      dark\r\n      color=\"#5c81b2\"\r\n      >\r\n          <v-spacer></v-spacer>\r\n          <v-divider\r\n            class=\"mx-4\"\r\n            inset\r\n            vertical\r\n          ></v-divider>\r\n          <v-btn\r\n          color=\"#cf2084\"\r\n          dark\r\n          class=\"mb-2\"\r\n          @click=\"filters()\"\r\n          >\r\n            Filtrer\r\n          </v-btn>\r\n      </v-app-bar>\r\n      <v-container>\r\n        <v-row>\r\n          <v-col\r\n          cols=\"3\">\r\n            <v-card>\r\n              <v-card-title style=\"text-align: center\">Filtres</v-card-title>\r\n              <v-divider></v-divider>\r\n              <v-card-text>\r\n                <v-date-picker\r\n                  v-model=\"dates\"\r\n                  color=\"#cf2084\"\r\n                  range\r\n                ></v-date-picker>\r\n                <v-autocomplete\r\n                  v-model=\"selectedRegions\"\r\n                  :items=\"regions\"\r\n                  item-text=\"nom\"\r\n                  outlined\r\n                  small-chips\r\n                  label=\"Région\"\r\n                  multiple\r\n                  color=\"#cf2084\"\r\n                  return-object\r\n                ></v-autocomplete>\r\n                <v-autocomplete\r\n                  v-model=\"selectedVendeurs\"\r\n                  :items=\"vendeurs\"\r\n                  item-text=\"nom\"\r\n                  outlined\r\n                  small-chips\r\n                  label=\"Vendeur\"\r\n                  multiple\r\n                  color=\"#cf2084\"\r\n                  return-object\r\n                ></v-autocomplete>\r\n              </v-card-text>\r\n            </v-card>\r\n          </v-col>\r\n          <v-col>\r\n            <v-card>\r\n              <v-card-title>\r\n                <v-card-title>Liste des ventes</v-card-title>\r\n                <v-spacer></v-spacer>\r\n                  <v-divider\r\n                    class=\"mx-4\"\r\n                    inset\r\n                    vertical\r\n                  ></v-divider>\r\n                <v-text-field\r\n                  v-model=\"search\"\r\n                  color=\"#cf2084\"\r\n                  append-icon=\"mdi-magnify\"\r\n                  label=\"Rechercher\"\r\n                  single-line\r\n                  hide-details\r\n                ></v-text-field>\r\n              </v-card-title>\r\n              <v-data-table\r\n                :page.sync=\"page\"\r\n                hide-default-footer\r\n                :sort-by=\"['dateFacturation']\"\r\n                :headers=\"headers\"\r\n                :search=\"search\"\r\n                :items=\"ventes\"\r\n                item-key=\"vente\"\r\n                class=\"elevation-2\"\r\n                :items-per-page=\"-1\"\r\n              >\r\n              <template\r\n              v-slot:item=\"{ item }\">\r\n                <tr style=\"text-align:left;\">\r\n                  <td>{{item.dateFacturation}}</td>\r\n                  <td>{{item.numero}}</td>\r\n                  <td>{{item.venId.prenom + ' ' + item.venId.nom}}</td>\r\n                  <td>{{item.regId.nom}}</td>\r\n                </tr>\r\n              </template>\r\n              </v-data-table>\r\n            </v-card>\r\n          </v-col>\r\n        </v-row>\r\n      </v-container>\r\n    </div>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\nimport customHeader from '../components/CustomHeader.vue'\r\nimport customLogin from '../components/CustomLogin.vue'\r\n\r\nexport default {\r\n  name: 'Home',\r\n\r\n  components: {\r\n    customHeader,\r\n    customLogin\r\n  },\r\n  data: () => ({\r\n    ventes: [],\r\n    defaultVentes: [],\r\n    ventesFiltered: [],\r\n    page: 1,\r\n    pageCount: 0,\r\n    search: '',\r\n    headers: [\r\n      {\r\n        text: 'Date de facturation',\r\n        value: 'dateFacturation'\r\n      },\r\n      { text: 'Numéro de la vente', value: 'numero' },\r\n      { text: 'Nom du vendeur', value: ('venId.nom' + 'venId.prenom') },\r\n      { text: 'Nom de la région', value: 'regId.nom' }\r\n    ],\r\n    dates: [],\r\n    regions: [],\r\n    selectedRegions: [],\r\n    vendeurs: [],\r\n    selectedVendeurs: []\r\n  }),\r\n  methods: {\r\n    getVentes () {\r\n      fetch('http://localhost:8080/api/home/',\r\n        {\r\n          method: 'GET',\r\n          headers: {\r\n            'Content-Type': 'application/json'\r\n          }\r\n        }).then(response => response.json())\r\n        .then(data => {\r\n          data.forEach(el => {\r\n            this.defaultVentes.push(el)\r\n            this.ventes.push(el)\r\n          })\r\n        })\r\n    },\r\n    getRegions () {\r\n      fetch('http://localhost:8080/api/home/region',\r\n        {\r\n          method: 'GET',\r\n          headers: {\r\n            'Content-Type': 'application/json'\r\n          }\r\n        }).then(response => response.json())\r\n        .then(data => {\r\n          data.forEach(el => {\r\n            this.regions.push(el)\r\n          })\r\n        })\r\n    },\r\n    getVendeurs () {\r\n      fetch('http://localhost:8080/api/home/vendeur',\r\n        {\r\n          method: 'GET',\r\n          headers: {\r\n            'Content-Type': 'application/json'\r\n          }\r\n        }).then(response => response.json())\r\n        .then(data => {\r\n          data.forEach(el => {\r\n            this.vendeurs.push(el)\r\n          })\r\n        })\r\n    },\r\n    filters () {\r\n      fetch('http://localhost:8080/api/home/filtre',\r\n        {\r\n          method: 'POST',\r\n          headers: {\r\n            'Content-Type': 'application/json'\r\n          },\r\n          body: JSON.stringify({\r\n            regions: this.selectedRegions,\r\n            vendeurs: this.selectedVendeurs\r\n          })\r\n        }).then(response => response.json())\r\n        .then(data => {\r\n          data.forEach(el => {\r\n            this.ventes.push(el)\r\n          })\r\n        })\r\n    }\r\n  },\r\n  mounted () {\r\n    if (this.$store.state.isConnected) {\r\n      this.getVentes()\r\n      this.getRegions()\r\n      this.getVendeurs()\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n"],"sourceRoot":"src/views"}]}